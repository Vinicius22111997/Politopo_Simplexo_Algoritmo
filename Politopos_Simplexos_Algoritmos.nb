       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "resto", "]"}], "==", "0"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"saida", "=", 
          RowBox[{"{", "simplexo", "}"}]}], ";"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"saida", "=", 
          RowBox[{"{", "simplexo", "}"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"saida", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"saida", ",", "resto"}], "]"}]}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "saida"}]}], 
