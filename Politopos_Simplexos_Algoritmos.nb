      RowBox[{"pparcial", "=", "p"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{"True", ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"Not", "[", 
            RowBox[{"EDimensaoCheia", "[", "pparcial", "]"}], "]"}], ",", 
           RowBox[{
            RowBox[{"resto", "=", "pparcial"}], ";", " ", 
