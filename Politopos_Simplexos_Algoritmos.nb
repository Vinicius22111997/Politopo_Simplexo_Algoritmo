                RowBox[{
                 RowBox[{
                  RowBox[{"Length", "[", 
                   RowBox[{"separasimplexo", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], "]"}], "<", "resto"}], 
                 ",", " ", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"resto", "=", 
                   RowBox[{"Length", "[", 
                    RowBox[{"separasimplexo", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], "]"}]}], ";", 
                  "\[IndentingNewLine]", 
                  RowBox[{"h", "=", 
                   RowBox[{"Length", "[", 
                    RowBox[{"separasimplexo", "[", 
                    RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";"}]}], 
                "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
             "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
