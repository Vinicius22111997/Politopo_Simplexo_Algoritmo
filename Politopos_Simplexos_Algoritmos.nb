   RowBox[{"EIndependente", "[", "p_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"matriz", ",", "i", ",", "saida"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"saida", "=", "False"}], ";", "\[IndentingNewLine]", 
