       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "p", "]"}], "==", "1"}], ",", 
        RowBox[{
         RowBox[{"saida", "=", "True"}], ";"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"Length", "[", 
              RowBox[{"p", "[", 
               RowBox[{"[", "1", "]"}], "]"}], "]"}], "+", "1"}], ">", 
            RowBox[{"Length", "[", "p", "]"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"matriz", "=", 
             RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
            "\[IndentingNewLine]", 
            RowBox[{"For", "[", 
             RowBox[{
              RowBox[{"i", "=", "2"}], ",", 
              RowBox[{"i", "<=", 
               RowBox[{"Length", "[", "p", "]"}]}], ",", 
              RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"matriz", "=", 
                RowBox[{"Append", "[", 
                 RowBox[{"matriz", ",", 
                  RowBox[{
                   RowBox[{"p", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], "-", 
                   RowBox[{"p", "[", 
                    RowBox[{"[", "1", "]"}], "]"}]}]}], "]"}]}], ";"}]}], 
             "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"MatrixRank", "[", "matriz", "]"}], "==", 
               RowBox[{
                RowBox[{"Length", "[", "p", "]"}], "-", "1"}]}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"saida", "=", "True"}], ";"}]}], "\[IndentingNewLine]",
              "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "saida"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{3.8843606297474127`*^9},
